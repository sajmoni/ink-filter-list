{
  "name": "ink-filter-list",
  "version": "0.0.1",
  "description": "Select an item in a filterable list",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r esbuild-runner/register ./src/index.tsx",
    "app": "npm run build && preview && node -r esbuild-runner/register ./app.tsx",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sajmoni/ink-filter-list.git"
  },
  "ava": {
    "require": [
      "esbuild-runner/register"
    ],
    "extensions": [
      "tsx"
    ]
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sajmoni/ink-filter-list/issues"
  },
  "homepage": "https://github.com/sajmoni/ink-filter-list#readme",
  "dependencies": {
    "ink": "3.2.0",
    "ink-text-input": "4.0.3",
    "react": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "ava": "4.1.0",
    "chalk": "4.0.0",
    "esbuild-runner": "2.2.1",
    "ink-testing-library": "2.1.0",
    "package-preview": "4.0.0",
    "prettier": "2.5.1",
    "strip-ansi": "6.0.0",
    "typescript": "4.6.2"
  }
}
