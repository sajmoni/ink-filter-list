{
  "name": "ink-filter-list",
  "version": "0.0.1",
  "description": "ink component to pick or search in a list",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "node -r esbuild-runner/register ./src/index.tsx",
    "app": "npm run build && preview && node -r esbuild-runner/register ./app.tsx",
    "release": "npm run build && np",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sajmoni/ink-filter-list.git"
  },
  "files": [
    "dist/"
  ],
  "ava": {
    "require": [
      "esbuild-runner/register"
    ],
    "extensions": [
      "tsx"
    ]
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true
  },
  "keywords": [
    "ink",
    "react",
    "list",
    "filter",
    "search"
  ],
  "author": "sajmoni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sajmoni/ink-filter-list/issues"
  },
  "homepage": "https://github.com/sajmoni/ink-filter-list#readme",
  "peerDependencies": {
    "ink": "3.x",
    "react": "17.x"
  },
  "dependencies": {
    "ink-text-input": "4.0.3"
  },
  "devDependencies": {
    "@types/node": "18.7.6",
    "@types/react": "17.0.39",
    "ava": "4.3.1",
    "chalk": "4.0.0",
    "esbuild-runner": "2.2.1",
    "ink": "3.2.0",
    "ink-testing-library": "2.1.0",
    "np": "7.6.2",
    "package-preview": "4.0.0",
    "prettier": "2.7.1",
    "react": "17.0.2",
    "strip-ansi": "6.0.1",
    "tsconfig-one": "0.0.2",
    "typescript": "4.7.4"
  }
}
